	var activeCounties = counties.filter(function (hs) {
		return !hs.inactive;
	});
	var inactiveCounties = counties.filter(function (hs) {
		return hs.inactive;
	});
	inactiveCounties.push(otherCounties);

	// drop shadows
	activeCounties.map(function (county, i) {
		county.paths.unshift({
			path: county.paths[0].path,
			defaults: _defaults.dropshadow
		});
	});

	// drop shadows
	hotspots.forEach(function (hs, i) {
		hs.paths.unshift({
			path: hs.paths[0].path,
			defaults: _defaults.dropshadow
		});
	});

	// define the regions array
	this.regions = [].concat(inactiveCounties, activeCounties, hotspots);
}

