	//	this.onClickHandler = function(url){
	//		alert(url);
	//	}

	/*************************
  *** region definitions ***
  **************************/

	// add default values if missing
	function addDefaultValuesToCityHotSpotPaths(hotspots) {
		return hotspots.map(function (hs) {
			var paths = hs.paths;

			// add foreground color
			paths[0].defaults = _defaults.hotspots.fg;

			// add background color
			paths[1].defaults = _defaults.hotspots.bg;

			return hs;
		});
	}

	function addDefaultValuesToCountyHotSpotPaths(hotspots) {
		return hotspots.map(function (hs) {
			var paths = hs.paths;

			paths[0].defaults = _defaults.county;
			return hs;
		});
	}

