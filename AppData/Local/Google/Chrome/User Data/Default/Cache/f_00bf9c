// the variable that will eventually contain all map specs
// See exports at the end
var data;

/**
 * @param opts {Object} for DI/testing
 * @param opts.hotspots {Array} alternative hotspots data
 */
function RegionData(opts) {
	opts = opts || {}; // avoid nullpointers in checks

	// the selector id of the element which will render the map
	this.selector = 'regionMap';

	this.dimensions = {
		// the original dimensions that the polygons are defined relative to
		width: 600,
		height: 800,
		// the final scale in which the map is rendered to
		scale: .4,
		left: -20,
		top: -5
	};

	// handler for on-region-click
	this.onClickHandler = function (rid, _json) {
		//updateRegion(rid, json);
		showHideSections(rid);
	};

	var _defaults = {
		dropshadow: {
			opacity: 0,
			fill: '#333',
			transform: 't0,0',
			'stroke-width': 0,
			over: {
				fill: '#333',
				opacity: .85,
				transform: 't0,-2',
				'stroke-width': 0
			}
		},
		county: {
			opacity: 1,
			cursor: 'pointer',
			fill: '#005a00',
			'stroke-width': 0,
			'stroke-opacity': 0,
			transform: 't0,0',
			over: {
				opacity: 1,
				fill: '#005a00',
				transform: 't0,-7',
				'stroke-width': 0,
				'stroke-opacity': 0
			}
		},
		hotspots: {
			bg: {
				cursor: 'pointer',
				fill: '#333',
				'stroke-opacity': 0,
				transform: 't0,0',
				over: {
					transform: 't0,-7',
					fill: '#005a00'
				}
			},
			fg: {
				cursor: 'pointer',
				fill: '#fff',
				'stroke-opacity': 0,
				transform: 't0,0',
				over: {
					transform: 't0,-7'
				}
			}
		}
	};

