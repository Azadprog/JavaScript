module.exports = RegionData;

},{"./counties-data.json":1,"./hotspots-cities-data.json":2}],5:[function(require,module,exports){
'use strict';

/*
 *	@filename: vector_map.js
 *	@version: 1.1
 *	@author: Flemming Lauritzen
 *	@company: Reaktor AS
 *	@web: www.reaktor.no
 *
 *	v.1.1
 *	- Fixed potential problem when changing from one active region to another
 */

// highlights a region (vector)
function highlight(region) {
    $.each(region.paths, function (i, el) {
        region.vector[i].animate(getAttributes(el.over, el.defaults.over), 250);
    });
}

function downlight(region) {
    $.each(region.paths, function (i, el) {
        region.vector[i].animate(getAttributes(el, el.defaults), 250);
    });
}

function showToolTip($el, region) {
    try {
        $el.find('.txt .name').text(region.name).end().find('.txt .url').text(region.url).end().stop(true, true).delay(300).fadeIn(250);
    } catch (err) {}
}

function hideToolTip($el) {
    $el.stop(true, true).delay(25).fadeOut(0);
}

function getAttributes(obj, def) {
    return $.extend({}, def, obj);
}

function VectorMap(opts) {
    // jQuery elements for the map, tooltip and active tooltip,
    // and also the Rapael paper and the current active region.
    var dimensions = opts.dimensions;

    // copy all settings
    $.extend(this, opts);

    // as soon as the DOM is ready we'll initialize the map
    $(document).ready(function () {
        this.initMap();

        // scale the map to the element's full dimensions
        this.paper.setViewBox(0, 0, dimensions.width, dimensions.height, true);
    }.bind(this));
};

VectorMap.prototype.initMap = function () {
    // jQuery element of the map
    var $map = $('#' + this.selector);

    // the tooltip jQuery element
    var $tt = $map.find('.tt.mouse');

    // instanciate the raphael paper
    var paper = Raphael(this.selector, String(Math.ceil(this.dimensions.width * this.dimensions.scale)), String(Math.ceil(this.dimensions.height * this.dimensions.scale)));
    this.paper = paper;

    function drawPath(obj) {
        return paper.path(obj.path).attr(getAttributes(obj, obj.defaults));
    }

    // go through every region data object and draw the polygons
    $.each(this.regions, function (key, el) {

        paper.setStart();

        el.vector = [];

        $.each(el.paths, function (i, path) {
            var v = drawPath(path);
            el.vector.push(v);
        });

        var region = paper.setFinish();

        // If the region is active, no interaction is applied to it.
        if (el.inactive) return;

        var bg = el.vector[0];

        // add hover and click events for the region vectors
        region.hover(
        // mouse over
        function () {
            if (this.activeRegion) {
                try {
                    clearTimeout(this.activeTimeout);
                } catch (err) {}
                downlight(this.activeRegion);
                //hideToolTip($att);
            }

            highlight(el);
            showToolTip($tt, el);
        }.bind(this),
        // mouse out
        function () {
            if (this.activeRegion) {
                this.activeTimeout = setTimeout(function () {
                    highlight(this.activeRegion);
                    //showToolTip($att, this.activeRegion);
                }.bind(this), 500);
            }

            downlight(el);
            hideToolTip($tt, el);
        }.bind(this)).click(function () {
            this.onClickHandler(el.id, el.json);
        }.bind(this));
    }.bind(this));

    // whenever the mouse moves over the map we'll move the tooltip accordingly
    // (placed just above the mouse cursor)
    $map.mousemove(function (e) {
        // retrieve the offset of the map element (current target)
        var offset = $(e.currentTarget).offset();

        // place the tooltip
        $tt.css({
            left: e.pageX - $tt.outerWidth() / 2 - offset.left,
            top: e.pageY - $tt.outerHeight() - 5 - offset.top
        });
    });
};

// a public function allowing for activation of a region
// (regardless of mouse hover)
VectorMap.prototype.activateRegion = function (param, type) {
    if (typeof type === 'undefined') type = 'url';

    // returns the region with the defined url
    var getRegion = function (param, type) {
        var ret = false;

        if (typeof param === 'undefined') return ret;

        $.each(this.regions, function (key, val) {
            if (val[type] === param) {
                ret = val;
                // break the for each iteration
                return false;
            }
        });

        return ret;
    }.bind(this);

    $(document).ready(function () {
        if (this.activeRegion) {
            try {
                clearTimeout(this.activeTimeout);
            } catch (err) {}
            downlight(this.activeRegion);
        }

        // retrieve the region
        this.activeRegion = getRegion(param, type);
        if (this.activeRegion) {
            // highlight it
            highlight(this.activeRegion);
        }
    }.bind(this));
};

module.exports = VectorMap;

},{}]},{},[3]);
